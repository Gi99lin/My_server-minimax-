#!/usr/bin/expect -f
set timeout 30
set ip [lindex $argv 0]
set password [lindex $argv 1]

# Check NPM logs for mrtc requests
set cmd "docker logs nginx-proxy-manager --tail=100 2>&1 | grep -i mrtc"

spawn ssh -o StrictHostKeyChecking=no root@$ip $cmd
expect {
    "password:" { send "$password\r"; exp_continue }
    eof
}
